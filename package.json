{
  "name": "webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "node_modules/.bin/ng",
    "clean": "rm -rf dist && rm -rf build/resources/main/static",
    "build:dev": "npm run clean && ng build webapp --configuration development",
    "build:prod": "npm run clean && ng build webapp --configuration production",
    "build": "yarn run build:prod",
    "merge:i18n": "node merge-i18n.js",
    "webapp:dev": "npm run merge:i18n && ng cache clean && ng serve webapp --poll=2000",
    "webapp:prod": "node dist/server/server.mjs",
    "start": "yarn run webapp:dev",
    "watch": "node_modules/.bin/ng build webapp --watch --configuration development",
    "test": "node_modules/.bin/ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "yarn@1.22.22",
  "dependencies": {
    "@angular/cdk": "21.0.3",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "21.0.3",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.1",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@ng-select/ng-select": "^21.1.2",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "express": "^5.1.0",
    "ngx-dropzone": "^3.1.0",
    "ngx-translate-multi-http-loader": "^20.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.1",
    "@angular/cli": "^21.0.1",
    "@angular/compiler-cli": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@types/uuid": "^11.0.0",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
